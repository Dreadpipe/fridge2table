{
	"env": {
		"browser": true,
		"es6": true,
		"node": true
	},
	"extends": "airbnb",
	"globals": {
		"Atomics": "readonly",
		"SharedArrayBuffer": "readonly"
	},
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 2018,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/jsx-filename-extension": 0,
		"react/prop-types": 0,
		"react/prefer-stateless-function": 0,
		"consistent-return": 0,
		"default-case": 0,
		"react/destructuring-assignment": 0,
		"no-mixed-spaces-and-tabs": 0,
		"no-tabs": 0,
		"react/jsx-indent": 0,
		"react/jsx-indent-props": 0,
		"global-require": 0
	}
}
